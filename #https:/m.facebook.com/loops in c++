#include<iostream>

using namespace std;
int main(){
      int a=3;
      int *b =&a;
//       cout<<b;
      cout<<"the address of a is"<<&a<<endl;
      cout<<"the address of a is"<<&b<<endl;


     cout<<"the value  address of b is"<<*b<<endl;     //these star will give u d value
      
     int**c =&b;
     cout<<"the value of b is"<<&b<<endl;
     cout<<"the value of b is"<<c<<endl;
     //pointer to pointer  

    // wat is ponter  pointer is data type which holds the addres of other variable
    return 0;
